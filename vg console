   "source": [
    "video = pd.read_csv('vg.csv')\n",
    "video = video.dropna(axis=0)\n",
    "video['Year_of_Release']=video['Year_of_Release'].astype(int)\n",
    "video.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "PS3=video[video['Platform']=='PS3']\n",
    "PS4=video[video['Platform']=='PS4']\n",
    "Wii=video[video['Platform']=='Wii']\n",
    "WiiU=video[video['Platform']=='WiiU']\n",
    "X360=video[video['Platform']=='X360']\n",
    "XOne=video[video['Platform']=='XOne']\n",
    "\n",
    "vg=pd.concat([PS3,PS4,Wii,WiiU,X360,XOne], join='inner')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "PS31=PS3.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "PS31['platform']='PS3'\n",
    "PS41=PS4.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "PS41['platform']='PS4'\n",
    "Wii1=Wii.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "Wii1['platform']='Wii'\n",
    "WiiU1=WiiU.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "WiiU1['platform']='WiiU'\n",
    "X3601=X360.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "X3601['platform']='X360'\n",
    "XOne1=XOne.groupby(['Year_of_Release'],as_index=False)['Year_of_Release'].agg({'Number(in millions of units)':'count'})\n",
    "XOne1['platform']='XOne'\n",
    "\n",
    "vg1=pd.concat([PS31,PS41,Wii1,WiiU1,X3601,XOne1], join='inner')"
   ]
  },
